.coords.longitude\n    });\n  };\n  const getWeather = async () => {\n    if (!coords.lat)\n      return;\n\n    const data = await axios\n      .post('/api/weather', { ...coords });\n    const A = data.data.data;\n    setWeather({\n      tempK: A.main.temp - 273.15,\n      main: A.weather[0].main,\n      description: A.weather[0].description,\n      country: A.sys.country,\n      city: A.name\n    });\n  };\n  const convertTemp = () => {\n    setWeather({\n      ...weather, tempK:getCOrF(weather.tempK)\n    })\n    setCels(prev => !prev);\n  }\n\n  const getCOrF = (temp) => {\n    if(cels) return temp * 9 / 5 + 32;\n    return (temp - 32) * 5 / 9;\n  }\n\n  return {\n    weather,\n    convertTemp,\n    icon\n  };\n\n};\n\nexport default useApplicationData1;",["21","22"],"/home/ian/WEB/DEC/fcc/src/components/App1.js",[],{"ruleId":"23","severity":1,"message":"24","line":33,"column":6,"nodeType":"25","endLine":33,"endColumn":14,"suggestions":"26"},{"ruleId":"27","replacedBy":"28"},{"ruleId":"29","replacedBy":"30"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getWeather'. Either include it or remove the dependency array.","ArrayExpression",["31"],"no-native-reassign",["32"],"no-negated-in-lhs",["33"],{"desc":"34","fix":"35"},"no-global-assign","no-unsafe-negation","Update the dependencies array to be: [coords, getWeather]",{"range":"36","text":"37"},[636,644],"[coords, getWeather]"]